<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PaymentSuccesful">

    <LinearLayout
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:id="@+id/r64mm8xoowdb"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rxlaz7x3rp5f_rl"
                    android:layout_width="match_parent"
                    android:layout_height="926dp"
                    >
                    <ImageView
                        android:id="@+id/rxlaz7x3rp5f"
                        android:layout_width="match_parent"
                        android:layout_height="926dp"
                        android:backgroundTint="#FFFFFF"
                        android:scaleType="fitXY"
                        />
                    <LinearLayout
                        android:paddingBottom="76dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">


                        <ImageView
                            android:id="@+id/rqbpezjpxfad"
                            android:layout_width="match_parent"
                            android:layout_height="260dp"
                            android:layout_marginHorizontal="61dp"
                            android:layout_marginBottom="47dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/berhasil" />
                        <TextView
                            android:id="@+id/rff9rasat23j"
                            android:textColor="#00A587"
                            android:textSize="30sp"
                            android:textStyle="bold"
                            android:layout_marginBottom="30dp"
                            android:layout_marginHorizontal="62dp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Transaksi Berhasil !!\n"
                            />
                        <TextView
                            android:id="@+id/rwxlwonvjv5"
                            android:textColor="#7C7C7C"
                            android:textSize="16sp"
                            android:layout_marginBottom="219dp"
                            android:layout_marginHorizontal="61dp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Pesanan anda sudah masuk kedalam\nantrian mohon ditunggu !!"
                            />

                        <LinearLayout
                            android:id="@+id/ray17ir85tb7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="32dp"
                            android:background="@drawable/cr19lr36000a28400ab8b"
                            android:clickable="true"
                            android:focusable="true"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:paddingVertical="24dp">

                            <TextView
                                android:id="@+id/ri0q672iz4i"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Next"
                                android:textColor="#FFF9FF"
                                android:textSize="24sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>